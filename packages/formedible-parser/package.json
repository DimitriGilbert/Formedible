{
  "name": "formedible-parser",
  "version": "0.3.10",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "build": "tsup src/index.ts --format esm,cjs --dts && rm -f public/r/formedible-parser.json && npx shadcn@latest build",
    "check-types": "npx tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest"
  },
  "keywords": [
    "form",
    "parser",
    "react",
    "typescript",
    "formedible",
    "validation"
  ],
  "author": "",
  "license": "MIT",
  "description": "Safe parser for Formedible form definitions - handles JSON, JavaScript object literals, and Zod schema expressions",
  "peerDependencies": {
    "@tanstack/react-form": "^1.19.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^4.0.5"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^26.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}